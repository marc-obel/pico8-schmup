__lua__
function _init() 
	player = {
		x=50,
		y=100
	}
end

-- comment!
function _draw()
	cls(15)
	--spr(1,player.x,player.y)
	sspr(8,7,12,9,player.x,player.y)
end

function _update60()
	if (btn(0)) player.x -=1
	if (btn(1)) player.x +=1
	if (btn(2)) player.y -=1
	if (btn(3)) player.y +=1
end
